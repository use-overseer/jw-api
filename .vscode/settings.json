{
  "editor.formatOnSave": true,
  "editor.formatOnPaste": true,
  "editor.codeActionsOnSave": { "source.fixAll.eslint": "explicit", "source.fixAll": "explicit" },
  "eslint.format.enable": true,
  "eslint.validate": ["javascript", "javascriptreact", "typescript", "typescriptreact", "vue"],
  "files.associations": { "*.css": "tailwindcss" },
  "i18n-ally.localesPaths": ["app/locales"],
  "i18n-ally.enabledFrameworks": ["vue"],
  "i18n-ally.sortKeys": true,
  "i18n-ally.keepFulfilled": true,
  "i18n-ally.keystyle": "nested",
  "tailwindCSS.classAttributes": ["class", "ui", "active-class", "inactive-class"],
  "tailwindCSS.experimental.classRegex": [["ui:\\s*{([^)]*)\\s*}", "(?:'|\"|`)([^']*)(?:'|\"|`)"]],
  "typescript.tsdk": "node_modules/typescript/lib",
  "i18n-ally.extract.ignored": ["*"],
  "i18n-ally.extract.ignoredByFiles": {
    "app\\pages\\events\\[slug].vue": [
